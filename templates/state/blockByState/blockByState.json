{
    "id": "fk-block-by-state",
    "className": "BlockByState",
    "name": "Block by state",
    "config": {
        "category": "Global state",
        "outputs": 1,
        "inputs": 1,
        "fields": [
            { "id": "state", "label": "State", "type": "text", "required": true, "dataType": "fk-state"},
            { "id": "invert", "label": "Invert", "type": "text", "required": true, "default": "false"}
        ],
        "onSave": "function () { this.name = $('#node-input-state option:selected').text(); }",
        "label": "function () { return this.name ? 'Block by ' + this.name : 'Block by state'; }",
        "color": "#FFCC00",
        "icon": "font-awesome/fa-database"
    },
    "help": "Help..."
}